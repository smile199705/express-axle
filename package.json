{
  "name": "express-axle",
  "version": "1.0.0",
  "description": "express项目基础框架模型",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node ./src/app.js",
    "dev_socket": "node ./src/websocket.js",
    "dev_dist": "node ./dist/app.js",
    "dev_socket_dist": "node ./dist/websocket.js",
    "lint-staged": "lint-staged",
    "start:dev": "nodemon app.js",
    "start": "pm2 start ./src/app.js --name=express-axle -f",
    "build": "pkg . -t -o ./lib/express-axle-websocket",
    "compile": "npx babel ./src --out-dir dist"
  },
  "bin": "./dist/websocket.js",
  "pkg": {
    "script": [
      "dev_socket_dist"
    ],
    "assert": [
      "/src",
      "/node_modules",
      "/lib",
      "configMap.json"
    ],
    "targets": [
      "node14-macos-arm64"
    ]
  },
  "type": "module",
  "author": "杨伟",
  "license": "ISC",
  "dependencies": {
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "days": "^1.1.1",
    "dmdb": "^1.0.15238",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nconf": "^0.12.0",
    "os": "^0.1.2",
    "pkg": "5.6.0",
    "pm2": "^5.3.0",
    "socket.io": "^4.6.1",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/plugin-transform-modules-commonjs": "^7.21.5",
    "@babel/preset-env": "^7.22.4",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.7",
    "silly-datetime": "^0.1.2",
    "typescript": "^5.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
